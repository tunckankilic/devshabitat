<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDevelopmentRegion</key>
	<string>$(DEVELOPMENT_LANGUAGE)</string>
	<key>CFBundleDisplayName</key>
	<string>Devshabitat</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>$(PRODUCT_BUNDLE_IDENTIFIER)</string>
	<key>CFBundleInfoDictionaryVersion</key>
	<string>6.0</string>
	<key>CFBundleName</key>
	<string>devshabitat</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleShortVersionString</key>
	<string>$(FLUTTER_BUILD_NAME)</string>
	<key>CFBundleSignature</key>
	<string>????</string>
	<key>CFBundleVersion</key>
	<string>$(FLUTTER_BUILD_NUMBER)</string>
	<key>LSRequiresIPhoneOS</key>
	<true/>
	<key>UILaunchStoryboardName</key>
	<string>LaunchScreen</string>
	<key>UIMainStoryboardFile</key>
	<string>Main</string>
	<key>UISupportedInterfaceOrientations</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>UISupportedInterfaceOrientations~ipad</key>
	<array>
		<string>UIInterfaceOrientationPortrait</string>
		<string>UIInterfaceOrientationPortraitUpsideDown</string>
		<string>UIInterfaceOrientationLandscapeLeft</string>
		<string>UIInterfaceOrientationLandscapeRight</string>
	</array>
	<key>CADisableMinimumFrameDurationOnPhone</key>
	<true/>
	<key>UIApplicationSupportsIndirectInputEvents</key>
	<true/>
	
	<!-- URL Schemes -->
	<key>CFBundleURLTypes</key>
	<array>
		<!-- Custom URL Scheme -->
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>devshabitat</string>
			</array>
			<key>CFBundleURLName</key>
			<string>com.example.devshabitat</string>
		</dict>
		<!-- Facebook Login - APP_ID değerini Facebook Developer Console'dan alın -->
		<dict>
			<key>CFBundleURLSchemes</key>
			<array>
				<string>fbAPP_ID</string>
			</array>
		</dict>
	</array>
	
	<!-- Camera & Photo Library Permissions -->
	<key>NSCameraUsageDescription</key>
	<string>Bu uygulama fotoğraf çekmek için kamera erişimi gerektirir</string>
	<key>NSPhotoLibraryUsageDescription</key>
	<string>Bu uygulama fotoğraf seçmek için galeri erişimi gerektirir</string>
	<key>NSPhotoLibraryAddUsageDescription</key>
	<string>Bu uygulama fotoğrafları kaydetmek için galeri erişimi gerektirir</string>
	
	<!-- Location Permissions -->
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Yakındaki geliştiricileri bulabilmek için konumunuza ihtiyacımız var.</string>
	<key>NSLocationAlwaysUsageDescription</key>
	<string>Arka planda konum güncellemeleri alabilmek için sürekli konum erişimine ihtiyacımız var.</string>
	<key>NSLocationAlwaysAndWhenInUseUsageDescription</key>
	<string>Yakındaki geliştiricileri bulabilmek ve arka planda konum güncellemeleri alabilmek için konumunuza ihtiyacımız var.</string>
	
	<!-- File Access -->
	<key>LSSupportsOpeningDocumentsInPlace</key>
	<true/>
	<key>UIFileSharingEnabled</key>
	<true/>
	
	<!-- Background Modes for Push Notifications -->
	<key>UIBackgroundModes</key>
	<array>
		<string>remote-notification</string>
		<string>background-fetch</string>
	</array>
	
	<!-- Network Security -->
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSAllowsLocalNetworking</key>
		<true/>
	</dict>
	
	<!-- Facebook Configuration - APP_ID değerini değiştirin -->
	<key>FacebookAppID</key>
	<string>APP_ID</string>
	<key>FacebookDisplayName</key>
	<string>Devshabitat</string>
	
	<!-- Query Schemes for Social Media -->
	<key>LSApplicationQueriesSchemes</key>
	<array>
		<string>fbapi</string>
		<string>fb-messenger-share-api</string>
		<string>fbauth2</string>
		<string>fbshareextension</string>
		<string>googlegmail</string>
		<string>googlemail</string>
	</array>
	
	<!-- Encryption Declaration -->
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	
	<!-- Prevent Screenshots in App Switcher (Optional Security) -->
	<key>UIApplicationSupportsMultipleScenes</key>
	<false/>
	
	<key>io.flutter.embedded_views_preview</key>
	<true/>
	
</dict>
</plist>